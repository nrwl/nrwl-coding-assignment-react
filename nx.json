{
  "implicitDependencies": {},
  "affected": {
    "defaultBase": "main"
  },
  "npmScope": "acme",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "ssr",
          "e2e",
          "swagger"
        ],
        "cacheDirectory": ".nxcache"
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "libsDir": "",
    "appsDir": ""
  },
  "defaultProject": "client",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  }
}
